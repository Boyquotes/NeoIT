_code = {
"fragment": "uniform texture tail_mask_1;
uniform texture tail_marking;

uniform color pelt;
uniform color tailtip;
uniform color marking;

vec3 col = pelt.rgb;

if(tex(tail_mask_1, UV).a > 0.5)
{
    col = tailtip.rgb;
}

if(tex(tail_marking, UV).a > 0.5)
{
    col = marking.rgb;
}

DIFFUSE = col;",
